{
  "name": "wechaty-ducks-contrib",
  "version": "0.0.1",
  "description": "Wechaty Ducks Contrib",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "clean": "shx rm -fr dist/*",
    "dist": "npm run clean && tsc",
    "example": "ts-node examples/ding-dong-bot.ts",
    "pack": "npm pack",
    "lint": "npm run lint:es && npm run lint:ts && npm run lint:md",
    "lint:md": "markdownlint README.md",
    "lint:ts": "tsc --noEmit",
    "redux-devtools": "redux-devtools --hostname=localhost --port=8000",
    "start": "ts-node --files examples/ding-dong-bot.ts",
    "test": "npm run lint && npm run test:unit",
    "test:pack": "bash -x scripts/npm-pack-testing.sh",
    "test:unit": "blue-tape -r ts-node/register \"src/**/*.spec.ts\" \"src/*.spec.ts\" \"tests/*.spec.ts\" \"tests/**/*.spec.ts\"",
    "lint:es": "eslint --ignore-pattern tests/fixtures/ '{bin,examples,scripts,src,tests}/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wechaty/wechaty-ducks-contrib.git"
  },
  "keywords": [
    "Wechaty",
    "Ducks",
    "Contrib",
    "Redux"
  ],
  "author": "Huan <zixia@zixia.net> (李卓桓)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wechaty/wechaty-ducks-contrib/issues"
  },
  "homepage": "https://github.com/wechaty/wechaty-ducks-contrib#readme",
  "devDependencies": {
    "@chatie/eslint-config": "^0.8.1",
    "@chatie/git-scripts": "^0.6.2",
    "@chatie/semver": "^0.4.7",
    "@chatie/tsconfig": "^0.10.1",
    "ducks": "^0.6.1",
    "pkg-jq": "^0.2.4",
    "tstest": "^0.4.10",
    "utility-types": "^3.10.0",
    "wechaty": "^0.40.1",
    "wechaty-puppet-mock": "^0.22.6",
    "wechaty-redux": "^0.2.7"
  },
  "peerDependencies": {},
  "dependencies": {
    "wechaty-redux": "^0.2.7",
    "typesafe-actions": "^5.1.0"
  },
  "git": {
    "scripts": {
      "pre-push": "npx git-scripts-pre-push"
    }
  },
  "publishConfig": {
    "tag": "next"
  }
}
